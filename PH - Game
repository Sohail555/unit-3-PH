class Character:
    def __init__(self, orignal, was_guessed=False):
        self.orignal = orignal
        self.was_guessed = was_guessed


    def given_character(self):
        for item in self.was_guessed:
            if item == self.orignal:
                return True
            else:
                return False

    def complie(self):
        if self.was_guessed:
            return self.original
        else:
            return "_"
