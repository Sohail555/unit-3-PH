class Character:
    def __init__(self, orignal, was_guessed=False):
        self.orignal = orignal
        self.was_guessed = was_guessed

    def character_match(self, guess):
        if guess == self.original:
            self.was_guessed = True

    def complie(self):
        if self.was_guessed:
            return self.original
        else:
            return "_"


